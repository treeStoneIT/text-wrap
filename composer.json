{
    "name": "treestoneit/text-wrap",
    "description": "A Laravel Nova Wrapped Text field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Treestoneit\\TextWrap\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Treestoneit\\TextWrap\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
